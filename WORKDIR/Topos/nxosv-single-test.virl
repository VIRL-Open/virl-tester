  content: |-
    #!/bin/sh
    ifconfig eth1 up 10.0.0.9 netmask 255.255.255.252
    route add -host 192.168.0.1 gw 10.0.0.10 dev eth1
    route add -net 10.0.0.4/30 gw 10.0.0.10 dev eth1
